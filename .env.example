# DelphiAgent environment configuration
# Copy this file to .env and fill in your keys.
# Never commit your real .env to version control.

# Required: OpenAI configuration
OPENAI_API_KEY=your_openai_api_key_here
# Optional: override model (default: gpt-4o)
OPENAI_MODEL=gpt-4o

# Required: Perplexity configuration
PERPLEXITY_API_KEY=your_perplexity_api_key_here
# Optional: override model (default: sonar-reasoning-pro)
PERPLEXITY_MODEL=sonar-reasoning-pro

# Optional: application logging level (not strictly required)
# LOG_LEVEL=info
